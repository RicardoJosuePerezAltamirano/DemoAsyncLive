<Window x:Class="app1Task.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:app1Task"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label x:Name="mensajes"></Label>
        <Button x:Name="start" Click="start_Click" Content="inicia" Width="100"></Button>
        <Button x:Name="stop" Click="stop_Click" Content="detener" Width="100"></Button>
        <Button x:Name="status" Click="status_Click" Content="status" Width="100"></Button>
        <Button x:Name="progressTest" Click="progressTest_Click"  Content="progreso" Width="100"></Button>
        

    </StackPanel>
</Window>
